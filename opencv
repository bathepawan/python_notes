    "import cv2\n",
    "import numpy as np\n",
    "\n",
    "data = np.ones([250,250,3], 'uint8')\n",
    "green = np.ones([250,250,3], 'uint8')\n",
    "print(data[0,0])\n",
    "green[:,:,1] = data[:,:,0] * (2**8-1)\n",
    "\n",
    "print(green.shape)\n",
    "cv2.imshow(\"Gree\", green)\n",
    "cv2.imwrite(r\"C:\\tmp\\green.jpg\", green)\n",
    "cv2.waitKey()\n",
    "cv2.destroyAllWindows()"
   ]
