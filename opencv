    "import cv2\n",
    "import numpy as np\n",
    "\n",
    "data = np.ones([250,250,3], 'uint8')\n",
    "blue = np.ones([250,250,3], 'uint8')\n",
    "print(data[0,0])\n",
    "blue[:,:,0] = data[:,:,0] * (2**8-1)\n",
    "\n",
    "print(blue.shape)\n",
    "cv2.imshow(\"Blue\", blue)\n",
    "cv2.imwrite(r\"C:\\tmp\\blue.jpg\", blue)\n",
    "cv2.waitKey()\n",
    "cv2.destroyAllWindows()"
   ]
  },
